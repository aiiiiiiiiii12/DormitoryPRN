@using ProjectPrn211.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model ProjectPrn211.Models.User

@{
	var user = ViewData["user"] as User;

}

<h1>Detail User:</h1>
<form id="myForm" asp-controller="Admin" asp-action="DEuser" method="post">
	<input asp-for="Account" placeholder="Account" value="@user.Account" readonly/>
	<input asp-for="Password" placeholder="Password" value="@user.Password"/>



	<input type="submit" value="Delete"/>
</form>
